{
  "name": "@usels/babel-plugin-legend-memo",
  "version": "0.0.1-beta.3",
  "description": "Babel plugin to auto-wrap Legend-State observable .get() calls in JSX with <Auto> component",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@babel/core": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-syntax-jsx": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@types/babel__core": "^7.20.0",
    "babel-plugin-tester": "^11.0.4",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.2.2"
  }
}
